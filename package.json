{
  "name": "@pika/build",
  "version": "0.1.0",
  "license": "MIT",
  "distributions": {
    "src": [
      true
    ],
    "plugins": [
      [
        "@pika/node-builder"
      ],
      [
        "@pika/types-builder"
      ],
